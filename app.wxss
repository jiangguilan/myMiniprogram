/* 文章场景相关 */
.page {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.textEllipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.textEllipsis2 {
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}

.block {
  background: #fff;
  border-radius: 20rpx;
  box-shadow: 0px 4px 20px rgba(12, 74, 114, 0.1);
  padding: 18rpx 24rpx;
  box-sizing: border-box;
  width: 100%;
}

.block.no-shadow {
  box-shadow: none;
}

.flex {
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  align-items: center;
}

.flex .inline-flex {
  display: inline-flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  align-items: center;
}

.flex .inline-flex .img {
  flex-shrink: 0;
}

.calc {
  width: auto;
}

.calc .reduce {
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
}

.calc input {
  display: inline-block;
  text-align: center;
  width: 100rpx;
  height: 48rpx !important;
  min-height: 48rpx;
  line-height: 48rpx;
  color: #0664A2;
  font-size: 32rpx;
  border: 1rpx solid #D5D5D5;
  margin: 0 12rpx;
}

.calc .plus {
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
}

.btn {
  width: 296rpx;
  height: 80rpx;
  border-radius: 20rpx;
  line-height: 80rpx;
  padding: 0;
  border: 1px solid #98A4B4;
  background: #fff;
  color: #98A4B4;
  margin: 18rpx 0 0;
  text-align: center;
}

.btn.on {
  border-color: #0C92F2;
  background: #0C92F2;
  color: #fff;
}

.bottom {
  position: fixed;
  bottom: 0;
  left: 50%;
  transform: translate(-50%, 0);
  background: #fff;
  width: 100%;
  height: 88rpx;
  padding: 0 0 0 32rpx;
  box-sizing: border-box;
  line-height: 88rpx;
  z-index: 99;
}

.btn-fill {
  width: 420rpx;
  height: 80rpx;
  background: #0C92F2;
  box-shadow: 0px 4rpx 12rpx rgba(0, 0, 0, 0.1);
  border-radius: 20rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
}

.btn-fill.grey {
  background: #D5D5D5;
}

.btn-fill.fail {
  color: #FF0652;
}

.border-top::before {
  position: absolute;
  right: 0;
  top: 0;
  left: 0;
  height: 1px;
  content: "";
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #EDEDED;
}

.border-bottom::after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 1px;
  content: "";
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #EDEDED;
}

.bottom-btn {
  position: fixed;
  width: 420rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #0C92F2;
  box-shadow: 0rpx 4rpx 12rpx rgba(0, 0, 0, 0.16);
  border-radius: 20rpx;
  bottom: calc(var(--window-bottom) + 32rpx);
  left: 50%;
  transform: translate(-50%, 0);
  text-align: center;
  font-size: 32rpx;
  color: #fff;
}

.swiper-box {
  padding: 20rpx;
  box-sizing: border-box;
  width: 100%;
}

.swiper-box .swiper {
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0rpx 4rpx 20rpx rgba(12, 74, 114, 0.1);
}

.swiper-box .swiper .slide-image {
  width: 100%;
  height: 100%;
}

view {
  width: 100%;
}
